{% extends 'layouts/app.html.twig' %}
{% block title 'Dossiers' %}
{% block content %}
  {% set links = [{name: 'Toutes vos recettes', url: path('recipe_list')}] %}
  {% set links = links|merge(folders|map(f => {name: f.name, url: path('folder_detail', {id: f.id}), detail: f.isIncludedInMenus ? 'Inclus dans les menus'})) %}
  {{ include('shared/_nav-list.html.twig', {links}, with_context=false) }}
{% endblock %}
